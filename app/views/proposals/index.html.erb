<p id="notice"><%= notice %></p>

<h1>Listing Proposals</h1>

<table>
  <thead>
    <tr>
      <th>Rfp amount</th>
      <th>Purchased 60</th>
      <th>Purchased 30</th>
      <th>Cpps</th>
      <th>Bonus</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proposals.each do |proposal| %>
      <tr>
        <td><%= proposal.rfp_amount %></td>
        <td><%= proposal.purchased_60 %></td>
        <td><%= proposal.purchased_30 %></td>
        <td><%= proposal.cpps %></td>
        <td><%= proposal.bonus %></td>
        <td><%= proposal.user %></td>
        <td><%= link_to 'Show', proposal %></td>
        <td><%= link_to 'Edit', edit_proposal_path(proposal) %></td>
        <td><%= link_to 'Destroy', proposal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proposal', new_proposal_path %>

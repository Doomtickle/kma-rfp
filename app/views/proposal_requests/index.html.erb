<p id="notice"><%= notice %></p>

<h1>Listing Proposal Requests</h1>

<table>
  <thead>
    <tr>
      <th>Client name</th>
      <th>Client</th>
      <th>Campaign name</th>
      <th>Basic description</th>
      <th>Flight date start</th>
      <th>Flight date end</th>
      <th>Staggered</th>
      <th>Goals and objectives</th>
      <th>Budget</th>
      <th>Targeting</th>
      <th>Geography</th>
      <th>Age group</th>
      <th>Gender</th>
      <th>Household income</th>
      <th>Interests</th>
      <th>Devices</th>
      <th>Devices description</th>
      <th>Creative ad units</th>
      <th>Day parting</th>
      <th>Day parting description</th>
      <th>Success metrics</th>
      <th>Specifications</th>
      <th>Insertion order terms</th>
      <th>Submission instructions</th>
      <th>Acceptable proposal formats</th>
      <th>Proposal due date</th>
      <th>Decision made by</th>
      <th>Selection criteria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proposal_requests.each do |proposal_request| %>
      <tr>
        <td><%= proposal_request.client_name %></td>
        <td><%= proposal_request.client_industry %></td>
        <td><%= proposal_request.campaign_name %></td>
        <td><%= proposal_request.basic_description %></td>
        <td><%= proposal_request.flight_date_start %></td>
        <td><%= proposal_request.flight_date_end %></td>
        <td><%= proposal_request.staggered %></td>
        <td><%= proposal_request.goals_and_objectives %></td>
        <td><%= proposal_request.budget %></td>
        <td><%= proposal_request.targeting1 %></td>
        <td><%= proposal_request.geography %></td>
        <td><%= proposal_request.age_group %></td>
        <td><%= proposal_request.gender %></td>
        <td><%= proposal_request.household_income %></td>
        <td><%= proposal_request.interests %></td>
        <td><%= proposal_request.devices %></td>
        <td><%= proposal_request.d_description %></td>
        <td><%= proposal_request.creative_ad_units %></td>
        <td><%= proposal_request.day_parting %></td>
        <td><%= proposal_request.day_parting_description %></td>
        <td><%= proposal_request.success_metrics %></td>
        <td><%= proposal_request.specifications %></td>
        <td><%= proposal_request.insertion_order_terms %></td>
        <td><%= proposal_request.submission_instructions %></td>
        <td><%= proposal_request.acceptable_proposal_formats %></td>
        <td><%= proposal_request.proposal_due_date %></td>
        <td><%= proposal_request.decision_made_by %></td>
        <td><%= proposal_request.selection_criteria %></td>
        <td><%= link_to 'Show', proposal_request %></td>
        <td><%= link_to 'Edit', edit_proposal_request_path(proposal_request) %></td>
        <td><%= link_to 'Destroy', proposal_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

